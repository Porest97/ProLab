@model RefFees

@{
    ViewData["Title"] = "EditRefFee";
}

<h1>Edit</h1>

<h4>RefFees</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditRefFee">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Category" class="control-label"></label>
                <input asp-for="Category" class="form-control" />
                <span asp-validation-for="Category" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UDZ" class="control-label"></label>
                <input asp-for="UDZ" class="form-control" />
                <span asp-validation-for="UDZ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HD" class="control-label"></label>
                <input asp-for="HD" class="form-control" />
                <span asp-validation-for="HD" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LD" class="control-label"></label>
                <input asp-for="LD" class="form-control" />
                <span asp-validation-for="LD" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-sm btn-outline-success" asp-action="IndexRefFees">Till Arvoden</a>
</div>