@model ProLab.Models.DataModels.PlanPost

@{
    ViewData["Title"] = "Delete";
}
<div class="card">
    <div class="card-header">
        <h6>Ta bort denna ?</h6>
    </div>
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-2">@Html.DisplayNameFor(model => model.DateTimePosted)</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.DateTimePosted)</dd>

            <dt class="col-sm-2">@Html.DisplayNameFor(model => model.DateTimeChanged)</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.DateTimeChanged)</dd>

            <dt class="col-sm-2">@Html.DisplayNameFor(model => model.ApplicationUser)</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.ApplicationUser.FullName)</dd>

            <dt class="col-sm-2">@Html.DisplayNameFor(model => model.PlanedDateTime)</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.PlanedDateTime)</dd>

            <dt class="col-sm-2">@Html.DisplayNameFor(model => model.StartDateTime)</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.StartDateTime)</dd>

            <dt class="col-sm-2">@Html.DisplayNameFor(model => model.EndDateTime)</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.EndDateTime)</dd>

            <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Aktivitet)</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.Aktivitet.AktivitetsBeskrivning)</dd>
        </dl>
    </div>
    <div class="card-footer">
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-sm btn-danger" />
            <a class="btn btn-sm btn-outline-success" asp-controller="Planerings" asp-action="IndexPlanPosts">Till Planeringen</a>
        </form>
    </div>
</div>