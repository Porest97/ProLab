@model Referee

@{
    ViewData["Title"] = "DetailsRef";
}
<div class="card" style="width:50%">
    <div class="card-header col-md-12">
        <h6>Information Domare</h6>
    </div>
    <div class="card-body col-md-12">
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FirstName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FirstName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LastName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LastName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.StreetAddress)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.StreetAddress)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ZipCode)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ZipCode)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.City)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.City)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Country)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Country)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Ssn)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Ssn)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PhoneNumber1)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PhoneNumber1)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PhoneNumber2)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PhoneNumber2)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PrivateEmail)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PrivateEmail)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ApplicationUser)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ApplicationUser.FullName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SwishNumber)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SwishNumber)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.BankAccount)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.BankAccount)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.BankName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.BankName)
            </dd>
        </dl>
    </div>

    <div class="card-footer col-md-12">
        <a class="btn btn-sm btn-success" asp-controller="Referee" asp-action="RefMain">Hockey Domare</a>
        <a class="btn btn-sm btn-success" asp-controller="Referee" asp-action="ListReferees">Lista Domare</a>
    </div>
</div>
